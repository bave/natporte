PROGNAME = natporte

.PHONY: all clean

utils: utils.cpp utils.hpp
		c++ -g -o utils utils.cpp

log: log.cpp log.hpp
		c++ -g -o log log.cpp

interface: interface.cpp interface.hpp
		c++ -g -o interface interface.cpp

structure: structure.cpp structure.h
		c++ -g -o structure structure.cpp

ring: ring.cpp ring.h
		c++ -Wall -g -o ring ring.cpp -lpthread

clean:
		rm -f *~ *.a *.o
		rm -rf *.dSYM
		rm -f ./a.out ./utils ./log ./interface ./ring ./structure
		rm -rf ./build
		rm -rf ../bin/node_module
		rm -rf ../bin/$(PROGNAME) 
		rm -rf ../bin/$(PROGNAME).core

netutils_js:
	make netutils_js

all:
	make

.DEFAULT: all

